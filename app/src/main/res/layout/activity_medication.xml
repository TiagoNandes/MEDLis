<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Medication">

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="208dp"
        android:layout_height="226dp"
        android:background="#FFCB00"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.257"
        app:srcCompat="@drawable/icon_metro_medkit" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="170dp"
        android:layout_height="58dp"
        android:text="Ler Posologia"
        android:textColor="#FFFFFF"
        android:textSize="24dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="288dp" />

    <TextView
        android:id="@+id/medName"
        android:layout_width="170dp"
        android:layout_height="58dp"
        android:fontFamily="@font/bahnschrift1"
        android:gravity="center_horizontal"
        android:text="Clavamox"
        android:textColor="#000000"
        android:textSize="24dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="402dp" />

    <TextView
        android:id="@+id/med_posology"
        android:layout_width="234dp"
        android:layout_height="107dp"
        android:gravity="center_horizontal"
        android:text="1 comprimido de 12 em 12 horas durante 7 dias"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="460dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="229dp"
        android:layout_height="62dp"
        android:gravity="center_horizontal"
        android:text="Consultar bula"
        android:textColor="#3F51B5"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="567dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="416dp"
        android:layout_height="90dp"
        android:layout_marginBottom="188dp"
        android:background="#1D71B8"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="65dp"
            android:layout_height="63dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.51"
            app:srcCompat="@drawable/arrow" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginLeft="12dp"
            android:text="Voltar ao menu"
            android:textColor="#FFFFFF"
            android:textSize="24sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView10"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>